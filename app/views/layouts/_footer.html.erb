<% if get_all_pages.size > 0 %>
  <% for o_row in get_all_pages %>
  	<% if o_row.page_route == "help" %>
			<%= link_to t("general.help"), :help %> |  		
  	<% else %>
  		<%= link_to o_row.name, "/other/#{o_row.page_route}" %> |	
  	<% end %>
  <% end %>
<% end %>
<%= link_to t("general.contact_us"), :contact_us %> |
<%= t("general.copyright_all_rights_reserved_html") %>




<div id="spinner" style="margin: 0px; left: 0px; display: none;" class="loaderimgText">
  <%= image_tag "loader.gif" %><br><div class="backTransparant"></div>
</div>

<script type="text/javascript">
	$(document).ready(function() {
		$("#spinner").hide();
	});
</script>